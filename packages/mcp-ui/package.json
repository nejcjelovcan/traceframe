{
  "name": "@nejcjelovcan/traceframe-mcp-ui",
  "version": "0.4.1",
  "type": "module",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "bin": {
    "mcp-ui": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx main.ts",
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config ../../.eslintrc.json \"**/*.ts\" --ignore-pattern \"dist/**\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix --config ../../.eslintrc.json \"**/*.ts\" --ignore-pattern \"dist/**\"",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\""
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcjelovcan/traceframe.git",
    "directory": "packages/mcp-ui"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.0",
    "glob": "^13.0.1",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "@nejcjelovcan/traceframe-ui-library": ">=1.0.0 <3.0.0"
  },
  "devDependencies": {
    "@nejcjelovcan/traceframe-ui-library": "workspace:^",
    "@types/node": "^22.19.11",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^4"
  }
}
