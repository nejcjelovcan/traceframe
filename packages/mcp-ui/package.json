{
  "name": "@traceframe/mcp-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "mcp-ui": "./dist/main.js"
  },
  "scripts": {
    "dev": "tsx main.ts",
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config ../../.eslintrc.json \"**/*.ts\" --ignore-pattern \"dist/**\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix --config ../../.eslintrc.json \"**/*.ts\" --ignore-pattern \"dist/**\"",
    "format": "prettier --cache --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --cache --check \"**/*.{ts,json,md}\""
  },
  "dependencies": {
    "@traceframe/ui-library": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.25.0",
    "glob": "^13.0.1",
    "playwright": "^1.58.1",
    "sharp": "^0.34.5",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
