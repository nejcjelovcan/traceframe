{
  "name": "@nejcjelovcan/traceframe-storybook-preset",
  "version": "1.3.3",
  "type": "module",
  "description": "Storybook preset for Traceframe theme integration",
  "main": "./preset.js",
  "exports": {
    ".": {
      "import": "./preset.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.js"
    },
    "./plugin": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "preset.js"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcjelovcan/traceframe.git",
    "directory": "packages/storybook-preset"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config ../../.eslintrc.json \"**/*.{ts,tsx}\" --ignore-pattern \"dist/**\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix --config ../../.eslintrc.json \"**/*.{ts,tsx}\" --ignore-pattern \"dist/**\"",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,js}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md,js}\""
  },
  "dependencies": {
    "@nejcjelovcan/traceframe-ui-library": "workspace:*"
  },
  "peerDependencies": {
    "@storybook/addon-themes": "^10.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@storybook/addon-themes": "^10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/ui": "^4.0.18",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.9.3",
    "vite": "^7",
    "vitest": "^4"
  }
}
