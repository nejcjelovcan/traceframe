{
  "name": "@nejcjelovcan/eslint-plugin-traceframe",
  "version": "0.3.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "description": "ESLint plugin with custom rules for Traceframe code quality",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcjelovcan/traceframe.git",
    "directory": "packages/eslint-plugin"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config ../../.eslintrc.json \"*.ts\" \"**/*.ts\" --ignore-pattern \"dist/**\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix --config ../../.eslintrc.json \"*.ts\" \"**/*.ts\" --ignore-pattern \"dist/**\"",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "@nejcjelovcan/traceframe-ui-library": "workspace:^",
    "@typescript-eslint/utils": "^8.18.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "@typescript-eslint/parser": "^8.19.1",
    "@typescript-eslint/rule-tester": "^8.18.1",
    "eslint": "^9.18.0",
    "typescript": "^5.9.3",
    "vitest": "^4"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  }
}
