{
  "name": "@nejcjelovcan/traceframe-ui-library",
  "version": "0.4.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fonts": {
      "types": "./dist/fonts.d.ts",
      "import": "./dist/fonts.js"
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind-preset": {
      "types": "./dist/tailwind-preset.d.ts",
      "import": "./dist/tailwind-preset.js",
      "default": "./dist/tailwind-preset.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch --project tsconfig.build.json",
    "build": "pnpm generate:component-metadata && tsc --build tsconfig.build.json && pnpm build:css",
    "generate:component-metadata": "node scripts/generate-component-metadata.js",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/styles.css",
    "generate:tokens": "node scripts/build-tokens.js",
    "validate:token-definitions": "node scripts/validate-token-definitions.js",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config ../../.eslintrc.json \"src/**/*.{ts,tsx}\" --ignore-pattern \"dist/**\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --fix --config ../../.eslintrc.json \"src/**/*.{ts,tsx}\" --ignore-pattern \"dist/**\"",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md,css}\""
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcjelovcan/traceframe.git",
    "directory": "packages/ui-library"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@fontsource-variable/ibm-plex-sans": "^5.2.8",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/space-grotesk": "^5.2.10",
    "@fontsource/ibm-plex-mono": "^5.2.7",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/space-mono": "^5.2.9",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-virtual": "^3.13.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@storybook/addon-themes": "^10",
    "@storybook/react-vite": "^10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^10",
    "style-dictionary": "^4.1.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
}
